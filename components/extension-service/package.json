{
  "name": "@cn-gitpod/extension-service",
  "version": "0.0.1",
  "description": "Typescript Express.js server for Extension Service",
  "scripts": {
    "p:deploy": "npx prisma migrate deploy",
    "p:generate": "npx prisma generate",
    "p:migrate": "npx prisma migrate dev",
    "p:studio": "npx prisma studio",
    "clean": "rimraf lib",
    "start": "node ./lib/app.js",
    "build": "yarn lint && npx tsc",
    "build:clean": "yarn clean && yarn build",
    "rebuild": "yarn build:clean",
    "lint": "yarn tslint -p .",
    "updatearm": "yarn rebuild && leeway build components/extension-service:docker -D version=arm-support --dont-test"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "typescript": "^4.9.4",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "prisma": "^4.8.1",
    "@cn-gitpod/extension-service-api": "0.0.1",
    "@grpc/grpc-js": "^1.8.1",
    "@grpc/proto-loader": "^0.7.4",
    "@prisma/client": "^4.8.1",
    "dotenv": "^16.0.3",
    "zod": "^3.20.2"
  }
}
